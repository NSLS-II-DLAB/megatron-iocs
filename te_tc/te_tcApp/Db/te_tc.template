record(fanout, "$(Sys)$(Dev)FOut1_")
{
        field(SELM, "All")
        field(LNK1, "$(Sys)$(Dev)T:SP-RB")
        field(LNK2, "$(Sys)$(Dev)Val:PID_P-RB")
        field(LNK3, "$(Sys)$(Dev)Val:PID_I-RB")
        field(LNK4, "$(Sys)$(Dev)Val:PID_D-RB")
        field(LNK5, "$(Sys)$(Dev)T:Offset-RB")
        field(LNK6, "$(Sys)$(Dev)FOut2_")
        field(SCAN, "2 second")
}

