# Get model
record(stringin, "$(Sys)$(Dev)Model-I") {
  field(DESC, "Model")
  field(DTYP, "stream")
  field(INP,  "@tetech-4820.proto read_str(00) $(PORT)")
  field(PINI, "1")
}

# Control sensor temperature
record(ai, "$(Sys)$(Dev)T:Cntl-I") {
  field(DESC, "Control Sensor T")
  field(DTYP, "stream")
  field(INP,  "@tetech-4820.proto read_val(01) $(PORT)")
  field(EGU,  "C")
  field(PREC, "2")
  field(ASLO, "0.1")
  field(LINR, "LINEAR")
  field(SCAN, "1 second")
  field(FLNK, "$(Sys)$(Dev)Val:PwrOut-I")
}

# Power output
record(ai, "$(Sys)$(Dev)Val:PwrOut-I") {
  field(DESC, "Power Output")
  field(DTYP, "stream")
