record(calcout, "Depo{GV:1}Pos-Calc")
{
	field(DESC, "{GV:1} Pos Calc")
	field(INPA, "Depo{GV:1}Sts:Stroke-Sts")
	field(INPB, "Depo{GV:1}Pos-Sts")
	field(CALC, "(A<<1)+B")
	field(OUT,  "Depo{GV:1}Pos:Calc-Sts PP")
	field(SCAN, "1 second")
}

record(mbbi, "Depo{GV:1}Pos:Calc-Sts")
{
	field(DESC, "{GV:1} Calc Pos")
	field(ZRST, "Fully Closed")
	field(ONST, "Error")
	field(TWST, "Closed Pos")
	field(THST, "Fully Open")
}
