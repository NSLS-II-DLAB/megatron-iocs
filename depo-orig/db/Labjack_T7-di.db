# bi record template for UNIT16 inputs
record(bi,"$(Sys)$(Dev)DI:01-Sts") {
    field(DTYP,"asynUInt32Digital")
    field(INP,"@asynMask($(LJPORT)_DI 0 0x1)")
    field(SCAN, "I/O Intr")
    field(ZNAM,"Low")
    field(ONAM,"High")
    field(ZSV,"NO_ALARM")
    field(OSV,"NO_ALARM")
}

